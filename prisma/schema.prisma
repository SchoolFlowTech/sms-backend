generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "windows", "darwin-arm64"]
}
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id        Int   @id @default(autoincrement())
  email     String   @unique
  name      String?
  password  String
  role      roles
}

enum roles {
  admin
  student
  teacher
  parent
}
